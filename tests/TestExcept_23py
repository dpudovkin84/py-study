while True:
    a=input('Enter number a:')
    b=input('Enter number b:')
    if a.isdigit() and b.isdigit():
        if int(b)==0:
            print('Can"t divide by zero')
        else:
            print('Result:',(int(a)/int(b))**2)
            break
    else:
        print('Enter only numbers')
